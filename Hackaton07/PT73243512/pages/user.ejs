<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('../partials/head'); %>
</head>
<body class="container">

<header>
    <%- include('../partials/header'); %>
</header>

<main>
    <div class="jumbotron">
        <h1>This is great</h1>
        <p>Welcome to templating using EJS</p>
        <h2>Variable</h2>
       
        <ul>
            <% mascots.forEach(function(users) { %>
                <li>
                    <strong><%= users.name %></strong>
                    representing <%= users.surname %>, born <%= users.birth_year %>
                </li>
            <% }); %>
        </ul>
    </div>

</main>

<footer>
    <%- include('../partials/footer'); %>
</footer>

</body>
</html>